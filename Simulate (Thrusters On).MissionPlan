    <ProjectCommands>
      <FreeForm enabled="false">
        <Label>Done Via API (comment out)</Label>
        <Bookmarks />
        <Breakpoints />
        <FreeFormScript><![CDATA[//comment out the following when not testing

//loads TLEs for testing
mySatellite1.LoadTLE("my_sat.tle", 0);
numSatellites = Satellites.LoadTLE("other_sats.tle");

//sets thrust duration
thrustDuration = 0.01;
simDuration = 0.1;
]]></FreeFormScript>
      </FreeForm>
      <FreeForm enabled="true">
        <Label>Create Objects and Variables</Label>
        <Bookmarks />
        <Breakpoints />
        <FreeFormScript><![CDATA[
TimeSpan startEpoch = mySatellite1.Epoch;
TimeSpan timeClosestApproach;

TimeSpanArray timesClosestApproach;

Variable hardBodyRadius = 50; //m
Variable rangeAtTCA; //km
Variable currentRangeS1S2; //km
Variable currentRangeRate; 
Variable prevRangeRate;
Variable probabilityCollision;
Variable color = ColorTools.Aqua;
Variable numberPOCs = 0;

Array satelliteApproached;
Array currentRangeRates(numSatellites);
Array prevRangeRates(numSatellites);
Array satsWithErrors;
StringArray sigProbData;
]]></FreeFormScript>
      </FreeForm>
      <FreeForm enabled="true">
        <Label>Set Up Spacecraft</Label>
        <Bookmarks />
        <Breakpoints />
        <FreeFormScript><![CDATA[
// set propogator type to SGP4 (to be used for all TLE based Spacecraft that aren't maneuvering)
For i=0 to (numSatellites-1) step 1;
	Satellites[i].SetPropagatorType("SGP4");
End;

// set propogator type to RK89 for maneuvering mySatellite
mySatellite1.SetPropagatorType("RK89");
// sets drag coefficient (was changed when TLE was loaded in)
mySatellite1.Cd = 2.2;

// add a tail to the Satellites
ViewWindow1.SetHistoryMode(Satellites.ObjectId, 1);

// set size scaling factor (for visualization)
For i = 0 to (numSatellites-1) step 1;
	Satellites[i].BodyScale = 1;
End;

// reports
Report "Initial State Vector:" to Console;
Report "Epoch: ", mySatellite1.Epoch.ConvertToCalendarDate() to Console;
Report "X: ", mySatellite1.X, " Y: ", mySatellite1.Y, " Z: ", mySatellite1.Z, " VX: ", mySatellite1.VX, " VY: ", mySatellite1.VY, " VZ: ", mySatellite1.VZ, " VMag: ", mySatellite1.VMag, " Mass:", mySatellite1.Mass to Console;
Report " " to Console;
Report "Simulating for", simDuration, " days with thruster on for the first", thrustDuration, " days" to Console;
Report "Mass:", mySatellite1.Mass, " kg" to Console;
Report "Thrust:", (mySatellite1.Thrusters[0] AsType Thruster).ThrusterC1, " N" to Console;
Report " Cd:", mySatellite1.Cd, " Cl:", mySatellite1.Cl, " Cr:", mySatellite1.Cr, " Drag Area:", mySatellite1.DragArea, " Lift Area:", mySatellite1.LiftArea, " SRP Area:", mySatellite1.SRPArea to Console;
Report "CG:", mySatellite1.CenterOfMass[0], " ", mySatellite1.CenterOfMass[1], " ", mySatellite1.CenterOfMass[2] to Console;
Report "MOI:", mySatellite1.MOI to Console;
Report " " to Console;
]]></FreeFormScript>
      </FreeForm>
      <FreeForm enabled="true">
        <Label>Propogate mySatellite Maneuver</Label>
        <Bookmarks />
        <Breakpoints />
        <FreeFormScript><![CDATA[Put mySatellite1 to Ephemeris1;
While (mySatellite1.ElapsedTime < TimeSpan.FromDays(thrustDuration));
	Maneuver mySatellite1 using FiniteBurn1;
	Put mySatellite1 to Ephemeris1;
End;


If (simDuration > thrustDuration);
	While (mySatellite1.ElapsedTime < TimeSpan.FromDays(simDuration-thrustDuration));
		Step mySatellite1;
		Put mySatellite1 to Ephemeris1;
	End;
End;

Report "Thrust simulation completed." to Console;
Report " " to Console;
Report "Final State Vector:" to Console;
Report "Epoch: ", mySatellite1.Epoch.ConvertToCalendarDate() to Console;
Report "X: ", mySatellite1.X, " Y: ", mySatellite1.Y, " Z: ", mySatellite1.Z, " VX: ", mySatellite1.VX, " VY: ", mySatellite1.VY, " VZ: ", mySatellite1.VZ, " VMag: ", mySatellite1.VMag, " Mass:", mySatellite1.Mass to Console;
]]></FreeFormScript>
      </FreeForm>
      <FreeForm enabled="true">
        <Label>Re-Setup for POC</Label>
        <Bookmarks />
        <Breakpoints />
        <FreeFormScript><![CDATA[Spacecraft mySatellite2(Ephemeris1);
Step mySatellite2 to (mySatellite2.Epoch == startEpoch);

// add Spacecraft2 to visualizaitons
ViewWindow1.AddObject(mySatellite2);

// add a tail and change color for Spacecraft2
ViewWindow1.SetHistoryMode(mySatellite2.ObjectId, 1);
ViewWindow1.SetObjectColor(mySatellite2.ObjectId,color);

// filter out mySatellite from the Satellites formation 
// (in theory the TLE that created the Satellites formation should contain mySpacecraft's TLE info)
For i = (numSatellites-1) to 0 step -1;
	If (Satellites[i].DisplayName == mySatellite1.DisplayName);
		Satellites.RemoveAt(i);
		numSatellites = Satellites.Count;
	End;
End;


Report " " to Console;
Report " " to Console;
Report " " to Console;
Report "Number of Satellites: ", (numSatellites+1)<5,5> to Console;]]></FreeFormScript>
      </FreeForm>
      <FreeForm enabled="true">
        <Label>Propogate Spacecrafts for POC</Label>
        <Bookmarks />
        <Breakpoints />
        <FreeFormScript><![CDATA[
//step all Spacecraft to same starting epoch (reference is starting epoch of Spacecraft2)
For i = 0 to (numSatellites-1) step 1;
	// Try block ensures that program doesn't crash if a satellite has a propogation error
	Try sending ErrorCount to errorCount;
		satsWithErrors.PushBack(i);
		Step Satellites[i] to (Satellites[i].Epoch == mySatellite2.Epoch);
		satsWithErrors.PopBack();
	End;
End;

// reports number of satellites that had a propogation error
// common propogation errors are: negative eccentricity, spacecraft crashes into the earth, 
If (errorCount > 0.0);
	Report "There was an issue with stepping ", errorCount<5,5>, " satellites to the start epoch. This is normal as long as it is a small number (about 1%) of satellites, and is likely due to them developing a negative eccentricity during propogation." to Console;
	errorCount = 0.0;
	// removes the satellites with propogation errors
	For i = (satsWithErrors.Dimension-1) to 0 step -1;
		Satellites.RemoveAt(satsWithErrors[i]);
	End;
	Report "Removed ", satsWithErrors.Dimension<5,5>, " satellites from the simulation." to Console;
	numSatellites = Satellites.Count;
	Report "New number of satellites being simulated: ", (numSatellites+1)<5,5> to Console;
End;

// set prevRangeRates to the range rate at start time to prevent potentially skipping the first time of closest approach (TCA)
For i = 0 to (numSatellites-1) step 1;
	prevRangeRates[i] = mySatellite2.RangeRate(Satellites[i]);
End;

// reset satsWithErrors to be reused to catch errors below
satsWithErrors.Clear();
satsWithErrors.Dimension = numSatellites;

Update ViewWindow1;
// propogate for thrust duration and track every TCA between a Satellite and Spacecraft2
While (mySatellite2.ElapsedTime < TimeSpan.FromDays(simDuration));
	
	//step Spacecraft2 (to be used as a reference epoch for stepping the Satellites)
	Step mySatellite2;
	
	errorCount = 0.0;
	// step all other satellites in formation
	For i = 0 to (numSatellites-1) step 1;
		// Try block ensures program doesn't crash if a satellite crashes into the earth
		Try sending ErrorCount to errorCount;
			satsWithErrors[i] = 1;
			Step Satellites[i] to (Satellites[i].Epoch == mySatellite2.Epoch);
			satsWithErrors[i] = 0;
		End;
	End;
	
	// for each satellite in the formation, check for range rate sign change to determine if a TCA has occured, and if so store the time and range
	For i = 0 to (numSatellites-1) step 1;
		// doesn't run for sats that threw a propagation error
		If (satsWithErrors[i] == 0);
			//update current RangeRate
			currentRangeRates[i] = mySatellite2.RangeRate(Satellites[i]);
		
			//if rangeRate flips signs to being positive, that means spacecrafts are now going apart, so epoch is near a TCA
			If ((currentRangeRates[i] > 0) and (prevRangeRates[i] < 0 ));
				timesClosestApproach.PushBack(mySatellite2.Epoch);
				satelliteApproached.PushBack(i);
			End;
	
			//update previous RangeRate
			prevRangeRates[i] = currentRangeRates[i];
		End;
	End;
	
	Update ViewWindow1;
End;

// reports number of satellites that had a propogation error
If (errorCount > 0.0);
	Report "There was an issue with stepping ", errorCount<5,5>, " satellites to the end epoch. This is normal as long as it is a very small number ( < 1%) of satellites, and is likely due to them developing a negative eccentricity during propogation." to Console;
	errorCount = 0.0;
End;

// turn off tails so visualization doesn't get weird when jumping to different epochs
ViewWindow1.SetHistoryMode(mySatellite2.ObjectId, 0);
ViewWindow1.SetHistoryMode(Satellites.ObjectId, 0);
mySatellite2.TailLength = 0;
For i = 0 to (numSatellites-1) step 1;
	Satellites[i].TailLength = 0;
End;

// calculate probability of collision at each TCA
For i = 0 to (timesClosestApproach.Dimension-1) step 1;
	// try block ensure any errors don't crash the program
	Try sending ErrorCount to errorCount;
		// Step spacecrafts to approx TCA
		Step mySatellite2 to (mySatellite2.Epoch == timesClosestApproach[i]);
		Step Satellites[satelliteApproached[i]] to (Satellites[satelliteApproached[i]].Epoch == timesClosestApproach[i]);
	
		// Compute the exact TCA (function finds a relative minimum, so is only really accurate when the spacecraft are already at the approximate TCA
		timeClosestApproach = mySatellite2.TCA(Satellites[satelliteApproached[i]], rangeAtTCA);
		
		// Step spacecrafts to exact TCA
		Step mySatellite2 to (mySatellite2.Epoch == timeClosestApproach);
		Step Satellites[satelliteApproached[i]] to (Satellites[satelliteApproached[i]].Epoch == timeClosestApproach);

		// Calculate the Pc at TCA
		probabilityCollision  = mySatellite2.ProbabilityOfCollision(Satellites[satelliteApproached[i]], hardBodyRadius);

		// only display if there is a non-zero probability of collision
		If (probabilityCollision>=0.0000000000000000000000000000000000000000000000000000001);
			numberPOCs++;
			Report " " to Console;
			Report " " to Console;
			Report " " to Console;
			Report "Satellite" to Console;
			Report Satellites[satelliteApproached[i]].DisplayName to Console;
			Report "TCA" to Console;
			Report timeClosestApproach.ConvertToCalendarDate to Console;
			Report "Distance at TCA" to Console;
			Report rangeAtTCA to Console;
			Report "Probability of collision" to Console;
			Report probabilityCollision<14,55> to Console;
			Update ViewWindow1;
		End;
		
		If (probabilityCollision > 0.01);
			significantProb++;
			sigProbData.PushBack(Satellites[satelliteApproached[i]].DisplayName);
			sigProbData.PushBack(timeClosestApproach.ConvertToCalendarDate);
			sigProbData.PushBack(rangeAtTCA.ToString());
			sigProbData.PushBack(probabilityCollision.ToString());
		End;
	End;
End;

