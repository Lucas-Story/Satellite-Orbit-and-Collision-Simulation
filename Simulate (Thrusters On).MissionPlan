		Report "Distance at TCA" to Console;
		Report sigProbData[i+2] to Console;
		Report "Probability of collision" to Console;
		Report sigProbData[i+3] to Console;
	End;
End;
Report " " to Console;
Report " " to Console;
Report " " to Console;

